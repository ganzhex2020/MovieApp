<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.phi.moviecloud.auth.mapper.RoleMapper">
    <select id="findByName" resultType="com.phi.moviecloud.auth.model.Role">
        select * from roles where name = #{roleName}
    </select>
</mapper>
